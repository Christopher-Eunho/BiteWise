{
    "manifest_version": 3,
    "name": "BiteWise",
    "version": "1.0",
    "description": "nwHack",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "options_page": "options_page.html",
    "background": {
      "service_worker": "eventPage.js"
    },
    "permissions": [
    "tabs",
    "bookmarks",
    "storage",
    "contextMenus"]   
  }
