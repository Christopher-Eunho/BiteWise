{
    "manifest_version": 2,
    "name": "Toy Extension",
    "version": "1.0",
    "description": "A prep for nwHack Hackathion",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "browser_action":{
        "default_icon": "icon16.png",
        "default_popup": "popup.html",
        "options_page": "options_page.html"
    },
    "permissions": [
        "storage"
    ] 
}
